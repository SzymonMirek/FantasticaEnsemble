import gallery2 from '../assets/galeria/gallery2.png';
import gallery3 from '../assets/galeria/gallery3.png';
import gallery4 from '../assets/galeria/gallery4.png';
import gallery5 from '../assets/galeria/gallery10.png';
import gallery6 from '../assets/galeria/gallery6.png';
import gallery7 from '../assets/galeria/gallery7.png';
import gallery8 from '../assets/galeria/gallery8.png';
import gallery9 from '../assets/galeria/gallery9.png';
