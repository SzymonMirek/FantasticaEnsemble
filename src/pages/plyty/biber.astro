<script>
  async function getShippingOptions(postalCode, weight) {
  const response = await fetch('/api/inpost-shipping.php', {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json',
    },
    body: JSON.stringify({ postalCode, weight })
  });
  return await response.json();
}
</script>
